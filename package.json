    "Electron", "quick", "start", "tutorial", "demo"
  ],
  "author": "杨庆 393978119",
  "license": "CC0-1.0",
  "dependencies": {
    "art-template": "^4.10.2",
    "cheerio": "^1.0.0-rc.2",
    "ejs": "^2.6.1",
    "getmac": "^5.20.0",
    "koa": "^2.5.3",
    "koa-art-template": "^1.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
    "mongodb": "^3.1.6",
    "mysql": "^2.16.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "electron": "^19.0.0",
    "electron-builder": "^21.2.0",
    "ava": "^0.18.0",
